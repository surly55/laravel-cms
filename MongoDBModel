use App\MongoDBModel;

// Create a new document
$item = new MongoDBModel([
    'name' => 'Sample Item',
    'price' => 19.99,
]);
$item->save();

// Retrieve documents
$items = MongoDBModel::where('price', '>', 10)->get();

// Update a document
$item = MongoDBModel::find(1);
$item->price = 24.99;
$item->save();

// Delete a document
$item = MongoDBModel::find(2);
$item->delete();
