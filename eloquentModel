use App\MongoDBModel;

// Create a new document
$item = new MongoDBModel([
    'name' => 'Sample Name',
    'description' => 'Sample Description',
    'age' => 31,
]);
$item->save();

// Retrieve documents
$items = MongoDBModel::where('age', '>', 10)->get();

// Update a document
$item = MongoDBModel::find(1);
$item->age = 32;
$item->save();

// Delete a document
$item = MongoDBModel::find(2);
$item->delete();
